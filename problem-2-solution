#include <bits/stdc++.h>
using namespace std;

int main(){
    char a;
    int n;
    cin>>a>>n;
    int total=0;
    int indx=0;
    int arr[n][n];
    for(int i=0;i<n;i++){
        int teamtotal=0;
        for(int j=0;j<n;j++){
                cin>>arr[i][j];
                teamtotal+=arr[i][j];
            }if(teamtotal>total){
                total=teamtotal;
                indx=i;
            }
        }cout<<indx<<endl;
    }
